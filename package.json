{
  "name": "vue-admin-template",
  "version": "4.2.1-alpha.1",
  "description": "A vue admin template with Element UI & axios & iconfont & permission control & lint",
  "license": "MIT",
  "author": "Pan <panfree23@gmail.com>",
  "files": [
    "dist",
    "icons.js",
    "mixins.js"
  ],
  "main": "dist/lib",
  "module": "dist/lib/index.esm.js",
  "scripts": {
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "bundle": "yarn run bundle:bili && yarn run bundle:poi && yarn run bundle:pack",
    "bundle:bili": "use-bili",
    "bundle:pack": "yarn pack",
    "bundle:poi": "poi --no-clear-console --production",
    "dev": "vue-cli-service serve",
    "fix": "pretty-quick",
    "lint": "use-eslint --ext .js,.vue src",
    "preview": "node build/index.js --preview",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "test": "npm run test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "test:unit": "jest --clearCache && vue-cli-service test:unit"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "@babel/preset-env": "^7.4.0",
    "@babel/runtime-corejs3": "~7.5.5",
    "axios": "0.18.1",
    "element-ui": "2.7.2",
    "normalize.css": "7.0.0",
    "path-to-regexp": "2.4.0",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/register": "^7.4.0",
    "@frontend-bindings/conventions-use-bili": "file:../packages/frontend-bindings-conventions-use-bili-1.0.0.tgz",
    "@frontend-bindings/conventions-use-eslint": "file:../packages/frontend-bindings-conventions-use-eslint-1.0.0.tgz",
    "@frontend-bindings/conventions-use-jest": "file:../packages/frontend-bindings-conventions-use-jest-1.0.0.tgz",
    "@frontend-bindings/conventions-use-prettier": "file:../packages/frontend-bindings-conventions-use-prettier-1.0.0.tgz",
    "@frontend-bindings/tools-bundling-helpers": "file:../packages/frontend-bindings-tools-bundling-helpers-1.0.0.tgz",
    "@vue/cli-plugin-babel": "3.6.0",
    "@vue/cli-plugin-eslint": "^3.9.1",
    "@vue/cli-plugin-unit-jest": "3.6.3",
    "@vue/cli-service": "3.6.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "bili": "^4.8.1",
    "chalk": "2.4.2",
    "connect": "3.6.6",
    "eslint": "5.15.3",
    "eslint-plugin-vue": "5.2.2",
    "html-webpack-plugin": "3.2.0",
    "js-cookie": "2.2.0",
    "mockjs": "1.0.1-beta3",
    "node-sass": "^4.12.0",
    "nprogress": "0.2.0",
    "poi": "^12.7.2",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-vue": "^5.0.1",
    "runjs": "^4.3.2",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "script-loader": "0.7.2",
    "serve-static": "^1.13.2",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.2",
    "vue": "2.6.10",
    "vue-router": "3.0.6",
    "vue-template-compiler": "2.6.10"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.4.0",
    "core-js": "^3.0.0",
    "vue-runtime-helpers": "^1.0.1"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  }
}
